{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "./build",
    "rootDir": "./",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "jsx": "react",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}